{
    "description": "unhide hidden Divs in nation.co.ke to read hidden content.",
    "manifest_version": 2,
    "name": "read Nation",
    "version": "1.0",
    "icons": {
        "32": "icons/unhide-32.png",
        "96": "icons/unhide-96.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://nation.africa/*"
            ],
            "js": [
                "readnation.js"
            ]
        }
    ]
}